 <link rel="stylesheet" href="./styles/css/style3.css" media="screen" title="no title" charset="utf-8">

<div class="wrapper fadeInDown">
  <div id="formContent">
    <!-- Tabs Titles -->

    
    <!-- Login Icon -->
    <div class="fadeIn first">
      <img src="./styles/images/login-icon-with-transparent-background.jpg" id="icon" alt="User Icon"/>
    </div>

    <!-- Login Form -->
    <form id="login-form"> 
      <input type="text" id="userEmail" class="fadeIn second" name="login" placeholder="login">
      <input type="text" id="userPassword" class="fadeIn third" name="login" placeholder="password">
      <input type="submit" class="fadeIn fourth" value="Log In" id="submitUserBtn">
    </form>

    <!-- Remind Passowrd -->
    <div id="formFooter">
      <a class="underlineHover" href="#">Forgot Password?</a>
    </div>

  </div>
</div>

